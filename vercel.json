{
     "version": 2,
     "builds": [
          {
               "src": "frontend/package.json",
               "use": "@vercel/static-build",
               "config": {
                    "distDir": "frontend/dist"
               }
          },
          {
               "src": "backend/server.js",
               "use": "@vercel/node"
          }
     ],
     "routes": [
          {
               "src": "/(.*)",
               "dest": "/backend/server.js"
          },
          {
               "src": "/(.*)",
               "dest": "/frontend/dist/$1"
          }
     ]
}